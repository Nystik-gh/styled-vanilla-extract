{
  "name": "styled-vanilla-extract",
  "version": "0.5.4",
  "description": "A 0-runtime styled-components-like API for Qwik using vanilla-extract.",
  "license": "MIT",
  "type": "module",
  "types": "./lib-types/index.d.ts",
  "exports": {
    "./qwik": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "types": "./lib-types/index.d.ts"
    },
    "./vite": {
      "import": "./lib/vite.js",
      "require": "./lib/vite.cjs",
      "types": "./lib-types/vite.d.ts"
    },
    "./qwik-styled": {
      "import": "./lib/qwik-styled.js",
      "require": "./lib/qwik-styled.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "lib-types",
    "qwik",
    "vite"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint .",
    "lint.ci": "lint-staged -q --diff origin/$BASE_REF",
    "test": "vitest run",
    "test.watch": "vitest watch",
    "release": "npm run build && np",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.1.5",
    "@types/eslint": "^8.40.0",
    "@types/node": "^20.2.5",
    "@types/stylis": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vanilla-extract/css": "^1.11.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qwik": "^1.1.5",
    "lint-staged": "^13.2.2",
    "np": "^8.0.2",
    "prettier": "^2.8.8",
    "stylis": "^4.2.0",
    "tsc-files": "github:wmertens/tsc-files#fixSpawn",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.31.4"
  },
  "peerDependencies": {
    "@builder.io/qwik": ">= 0.14.1",
    "@vanilla-extract/css": "^1.9.1",
    "postcss": "^8.3.6",
    "postcss-load-config": "^3.1.0",
    "vite": "^3.2.3 || ^4"
  },
  "peerDependenciesMeta": {
    "postcss": {
      "optional": true
    },
    "postcss-load-config": {
      "optional": true
    }
  },
  "dependencies": {
    "@vanilla-extract/integration": "^6.2.1",
    "outdent": "^0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wmertens/styled-vanilla-extract.git"
  },
  "bugs": {
    "url": "https://github.com/wmertens/styled-vanilla-extract/issues"
  },
  "lint-staged": {
    "*.(md|json)": "prettier --list-different",
    "*.ts?(x)": "tsc-files --noEmit --allowJs false --checkJs false",
    "*.[tj]s?(x)": "eslint"
  }
}
