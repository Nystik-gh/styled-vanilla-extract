{
  "name": "styled-vanilla-extract",
  "version": "0.5.10",
  "description": "A 0-runtime styled-components-like API for Qwik using vanilla-extract.",
  "license": "MIT",
  "type": "module",
  "types": "./lib-types/index.d.ts",
  "qwik": "./lib/index.mjs",
  "exports": {
    "./qwik": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./lib-types/index.d.ts"
    },
    "./vite": {
      "import": "./lib/vite.mjs",
      "require": "./lib/vite.cjs",
      "types": "./lib-types/vite.d.ts"
    },
    "./qwik-styled": {
      "import": "./lib/qwik-styled.qwik.mjs",
      "require": "./lib/qwik-styled.qwik.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "lib-types",
    "qwik",
    "vite"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly  && prettier --write lib-types",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint . && tsc --noEmit && prettier --check .",
    "test": "vitest run",
    "test.watch": "vitest watch",
    "release": "npm run build && np",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.12",
    "@types/eslint": "^8.44.3",
    "@types/node": "^20.7.0",
    "@types/stylis": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vanilla-extract/css": "^1.13.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-qwik": "^1.2.12",
    "np": "^8.0.4",
    "prettier": "^3.0.3",
    "stylis": "^4.3.0",
    "tsc-files": "github:wmertens/tsc-files#fixSpawn",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.5"
  },
  "peerDependencies": {
    "@builder.io/qwik": ">= 0.14.1",
    "@vanilla-extract/css": "^1.9.1",
    "postcss": "^8.3.6",
    "postcss-load-config": "^3.1.0",
    "vite": "^3.2.3 || ^4"
  },
  "peerDependenciesMeta": {
    "postcss": {
      "optional": true
    },
    "postcss-load-config": {
      "optional": true
    }
  },
  "dependencies": {
    "@vanilla-extract/integration": "^6.2.2",
    "outdent": "^0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wmertens/styled-vanilla-extract.git"
  },
  "bugs": {
    "url": "https://github.com/wmertens/styled-vanilla-extract/issues"
  }
}
