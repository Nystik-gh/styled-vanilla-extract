{
  "name": "styled-vanilla-extract",
  "version": "0.5.3",
  "description": "A 0-runtime styled-components-like API for Qwik using vanilla-extract.",
  "repository": {
    "url": "https://github.com/wmertens/styled-vanilla-extract"
  },
  "exports": {
    "./qwik": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "types": "./lib-types/index.d.ts"
    },
    "./vite": {
      "import": "./lib/vite.js",
      "require": "./lib/vite.cjs",
      "types": "./lib-types/vite.d.ts"
    },
    "./qwik-styled": {
      "import": "./lib/qwik-styled.js",
      "require": "./lib/qwik-styled.cjs"
    }
  },
  "files": [
    "lib",
    "lib-types",
    "qwik",
    "vite"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "private": false,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint .",
    "test": "vitest run",
    "test.watch": "vitest watch",
    "release": "npm run build && np",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^0.15.2",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.13",
    "@types/stylis": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vanilla-extract/css": "^1.9.2",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qwik": "^0.15.2",
    "np": "^7.6.2",
    "prettier": "^2.8.1",
    "stylis": "^4.1.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.7"
  },
  "peerDependencies": {
    "@builder.io/qwik": ">= 0.14.1",
    "@vanilla-extract/css": "^1.9.1",
    "postcss": "^8.3.6",
    "postcss-load-config": "^3.1.0",
    "vite": "^3.2.3 | ^4"
  },
  "peerDependenciesMeta": {
    "postcss": {
      "optional": true
    },
    "postcss-load-config": {
      "optional": true
    }
  },
  "dependencies": {
    "@vanilla-extract/integration": "^6.0.1",
    "outdent": "^0.8.0"
  }
}
